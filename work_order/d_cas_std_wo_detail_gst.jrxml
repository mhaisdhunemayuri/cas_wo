	<parameter name="lhr_purc_order" class="java.lang.String"/>
	<queryString>
		<![CDATA[Select
	rate
	,quantity
	,line_no
	,purc_order
	,ACTIVITY_DESCR
	,unit
	,tot_amt
	,curr_code
	, pack_descr
	,unit_descr
	,HSN_NO
	,ITEM_CODE
	,DELIVERY_DATE
    ,SGST_RATE
    ,CGST_RATE
    ,IGST_RATE
	,(SGST_RATE+CGST_RATE+IGST_RATE) AS GST_RATE
    ,SGST_AMT
    ,CGST_AMT
    ,IGST_AMT
	,(SGST_AMT+CGST_AMT+IGST_AMT) AS GST_AMT
	,SHELF_LIFE
from (
SELECT porddet.rate
	,porddet.quantity
	,cast(porddet.LINE_NO as INT) as line_no
	,porddet.purc_order
	,item.descr
	,porddet.unit
	,porddet.tot_amt
	,porder.curr_code
	,packing.descr pack_descr
	,(
		SELECT sh_descr
		FROM uom
		WHERE unit = porddet.unit
		) AS unit_descr
	,ITEM.HSN_NO AS HSN_NO
	,ITEM.ITEM_CODE AS ITEM_CODE
	,porddet.dlv_date AS DELIVERY_DATE
    ,act.descr as ACTIVITY_DESCR
	,nvl(FN_RGET_TAX_NEW('P-ORD', porddet.purc_order, cast(porddet.line_no AS CHAR(3)), 'SGST_TAX_PVOUCH', ' ', ' ', 'P'), 0) AS SGST_RATE
    ,nvl(FN_RGET_TAX_NEW('P-ORD', porddet.purc_order, cast(porddet.line_no AS CHAR(3)), 'CGST_TAX_PVOUCH', ' ', ' ', 'P'), 0) AS CGST_RATE
    ,nvl(FN_RGET_TAX_NEW('P-ORD', porddet.purc_order, cast(porddet.line_no AS CHAR(3)), 'IGST_TAX_PVOUCH', ' ', ' ', 'P'), 0) AS IGST_RATE
	,nvl(FN_RGET_TAX_NEW('P-ORD', porddet.purc_order, cast(porddet.line_no AS CHAR(3)), 'SGST_TAX_PVOUCH', ' ', ' ', 'T'), 0) AS SGST_AMT
    ,nvl(FN_RGET_TAX_NEW('P-ORD', porddet.purc_order, cast(porddet.line_no AS CHAR(3)), 'CGST_TAX_PVOUCH', ' ', ' ', 'T'), 0) AS CGST_AMT
    ,nvl(FN_RGET_TAX_NEW('P-ORD', porddet.purc_order, cast(porddet.line_no AS CHAR(3)), 'IGST_TAX_PVOUCH', ' ', ' ', 'T'), 0) AS IGST_AMT
	,ITEM.SHELF_LIFE AS SHELF_LIFE

FROM porder
	,porddet
    ,activity act
    ,item
  LEFT OUTER JOIN packing ON item.pack_code = packing.pack_code
--LEFT OUTER JOIN activity a on porddet.ITEM_CODE = a.ITEM_CODE
WHERE (porder.purc_order = porddet.purc_order)
	AND (porddet.item_code = item.item_code)
    AND (porddet.ITEM_CODE = act.ITEM_CODE)
	AND ((COALESCE(porddet.quantity, 0) <> 0))
    and ( porddet.purc_order = '$P!{lhr_purc_order}' )

   -- order by cast(porddet.LINE_NO as INT)asc
) tab1]]>
	</queryString>
