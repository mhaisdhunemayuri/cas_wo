	<queryString>
		<![CDATA[SELECT
--ACTIVITY_CODE__PARENT,
a.purc_order as po_no,
a.ord_date as po_date,
a.ORD_AMT ,
fn_numbertoword(a.ORD_AMT) AMOUNTINWORD,
a.SITE_CODE__ORD,
b.DESCR as site_desc,
b.ADD1 as site_add1,
b.ADD2 as site_add2,
b.CITY as site_city,
b.UDF1 as sector_no,
upper(c.DESCR) as fin_descr,
c.ADDR1 as fin_add1,
c.ADDR2 as fin_add2,
c.ADDR3 as fin_add3,
c.CITY as fin_city,
c.PIN as fin_pin,
c.TELE1 as fin_tele1,
d.SUPP_NAME,
d.SUPP_CODE,
d.ADDR1,
d.ADDR2,
d.ADDR3,
d.CITY,
d.PIN,
d.PAN_NO,
case when d.CONT_PERS is null then ''
when d.CONT_PFX is null then nvl(d.CONT_PERS,'') else (nvl(d.CONT_PFX,'') || ' ' || nvl(d.CONT_PERS,'')) end AS CONT_PERS,

d.TELE1,
d.EMAIL_ADDR,
d.CST_NO,
d.TAX_REG_2,
e.QUOT_DATE,
a.TERM_TABLE,
a.udf_str__1 as hold_per,
to_char(e.QUOT_DATE, 'ddth Mon yyyy') as QUOT_DATE_new,
--LISTAGG(distinct FLD_NAME,',') as
nvl(g.descr,'') as activity,
min(f.start_date) as schedule_start_date,
max(f.end_date) as schedule_end_date,
h.rel_amt ret_perc,
--i.DESCR as DEPT_NAME,
(SELECT descr FROM GENCODES WHERE MOD_NAME='W_PORDER' AND FLD_NAME='UDF_STR__1' and fld_value = a.UDF_STR__1) as DEPT_NAME_new,
(SELECT descr FROM GENCODES WHERE MOD_NAME='W_PORDER' AND FLD_NAME='UDF_STR__2' and fld_value = a.UDF_STR__2) as supplier_prod,
s.pf_reg_No as PF_NO,
round(h.rel_after/30,0) as no_of_mnth
FROM PORDER a
inner join DEPARTMENT i on a.DEPT_CODE= i.DEPT_CODE
inner join site b on a.SITE_CODE__ORD=b.SITE_CODE
inner join finent c on b.FIN_ENTITY=c.FIN_ENTITY
inner join SUPPLIER d on a.SUPP_CODE=d.SUPP_CODE
left join SUPPLIER_OTHER_INFO s on d.SUPP_CODE = s.SUPP_CODE
left join PQUOT_HDR e on e.QUOT_NO = a.QUOT_NO
INNER JOIN PROJ_EST_BSL_ITEM f on a.PURC_ORDER=f.PURC_ORDER
INNER join GENCODES g on f.ACTIVITY_CODE__PARENT = g.FLD_VALUE
--INNER join Activity g on f.ACTIVITY_CODE__PARENT = g.ACTIVITY_PARENT
left join pord_pay_term h on h.rel_agnst='04' and a.purc_order=h.purc_order
where a.PORD_TYPE='JW'
and ACTIVITY_CODE__PARENT is not null
and  FLD_NAME='ACTIVITY_PARENT' AND MOD_NAME='ACTIVITY'
/*and a.SITE_CODE__ORD = 'S0008'
and a.ord_date >= '08-SEP-2021'
and a.ord_date <= '15-SEP-2021'
and a.purc_order >='08P6G0002'
and a.purc_order <='08P6G0002'*/
and a.SITE_CODE__ORD = '$P!{site_code}'
and a.ord_date >= $P{ord_date_fr}
and a.ord_date <= $P{ord_date_to}
and a.purc_order >='$P!{purc_order_fr}'
and a.purc_order <='$P!{purc_order_to}'
and d.SUPP_CODE >= '$P!{supplier_code_fr}'
AND d.SUPP_CODE <='$P!{supplier_code_to}'
group by a.purc_order ,
a.ord_date ,
a.ORD_AMT ,
a.SITE_CODE__ORD,
b.DESCR ,
b.ADD1 ,
b.ADD2 ,
b.CITY ,
b.UDF1 ,
c.DESCR ,
c.ADDR1 ,
c.ADDR2 ,
c.ADDR3 ,
c.CITY ,
c.PIN ,
c.TELE1 ,
d.SUPP_NAME,
d.SUPP_CODE,
d.ADDR1,
d.ADDR2,
d.ADDR3,
d.CITY,
d.PIN,
d.PAN_NO,
d.CONT_PERS,
d.CONT_PFX,
d.TELE1,
d.EMAIL_ADDR,
d.CST_NO,
d.TAX_REG_2,
e.QUOT_DATE,a.TERM_TABLE,
h.rel_amt,
a.udf_str__1,
UDF_STR__2,
h.rel_after,
i.DESCR,
g.descr,
s.pf_reg_No]]>
	</queryString>
